<?xml version="1.0"?>
<extension key="org.nubay.contributiondetailmod" type="module">
  <file>contributiondetailmod</file>
  <name>Modified Contribution Detail Report</name>
  <description>This extension adds a report template to fix the soft-credit issues in the core contribution report. Install the extension, create a new report from the new template and use it as you would the core contribution detail report</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Eli Lisseck</author>
    <email>lisseck@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page"></url>
    <url desc="Documentation"></url>
    <url desc="Support"></url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-02-22</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.x</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Contributiondetailmod</namespace>
  </civix>
</extension>
